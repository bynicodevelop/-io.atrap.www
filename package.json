{
  "private": true,
  "scripts": {
    "dev": "cp .env.development .env && nuxi dev",
    "build": "nuxi build",
    "clean": "rm -rf .nuxt/ .output/ node_modules/ .firebase/",
    "preset": "NITRO_PRESET=firebase yarn build",
    "local": "cp .env.development .env && yarn clean && yarn && yarn preset && firebase emulators:start",
    "deploy": "cp .env.production .env && yarn clean && yarn && yarn preset && firebase deploy",
    "emulator": "firebase emulators:start",
    "start": "node .output/server/index.mjs"
  },
  "devDependencies": {
    "nuxt3": "latest"
  },
  "dependencies": {
    "@headlessui/vue": "^1.4.3",
    "@heroicons/vue": "^1.0.5",
    "@nuxt/postcss8": "^1.1.3",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "aio-vue-analytics": "https://github.com/bynicodevelop/aio-analytics#main",
    "autoprefixer": "^10.4.2",
    "firebase": "^9.6.4",
    "firebase-admin": "^10.0.1",
    "firebase-functions": "^3.16.0",
    "mailchimp-api-v3": "^1.15.0",
    "nanoid": "^3.2.0",
    "postcss": "^8.4.5",
    "slugify": "^1.6.5",
    "tailwindcss": "^3.0.15",
    "yup": "^0.32.11"
  }
}
