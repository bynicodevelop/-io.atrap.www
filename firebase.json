{
  "hosting": {
    "public": ".output/public",
    "target": "www",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "function": "server"
      }
    ]
  },
  "functions": {
    "source": ".output/server"
  },
  "emulators": {
    "functions": {
      "port": 5004
    },
    "hosting": {
      "port": 5003
    },
    "ui": {
      "enabled": true
    }
  }
}
